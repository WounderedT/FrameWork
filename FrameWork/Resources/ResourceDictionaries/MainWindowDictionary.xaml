<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:FrameWork"
                    
                    xmlns:viewModel="clr-namespace:FrameWork.ViewModel"
                    xmlns:view="clr-namespace:FrameWork.UC">

    <DataTemplate DataType="{x:Type viewModel:DefaultTabViewModel}">
        <view:DefaultTabView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:CheckPasswordViewModel}">
        <view:CheckPasswordView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:SettingsViewModel}">
        <view:SettingsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:TabCloseViewModel}">
        <view:TabCloseView/>
    </DataTemplate>

</ResourceDictionary>